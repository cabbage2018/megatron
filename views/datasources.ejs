<!-- views/datasources.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ./partials/head %>
    <title><%= title %></title>
</head>
<body class="container">
    <header>
        <% include ./partials/header %>
    </header>
    <main>
        
        <span> --- </span>

        <div>
            <table class="table table-hover usertable">
                <tr>
                    <th>host</th>
                    <th>port</th>
                    <th>interval Seconds</th>
                    <th>functioncode</th>
                    <th>position</th>
                    <th>counts</th>
                    <th>lastupdate</th>
                </tr>
                <%for(var key in items){%>
                <tr>
                    <td><%=items[key].host%></td>
                    <td><%=items[key].port%></td>
                    <td><%=items[key].intervalSeconds%></td>
                    <td><%=items[key].functioncode%></td>
                    <td><%=items[key].position%></td>
                    <td><%=items[key].counts%></td>
                    <td><%=items[key].lastupdate%></td>
                </tr>
                <%}%>
        
            </table>
        </div>

        <span> --- </span>
        
        <span> --- </span>
        
        <h1>
            add <! % = items.length %>
        </h1>
    </main>
    <footer>
        <% include ./partials/footer %>
    </footer>
</body>
</html>


