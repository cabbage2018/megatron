{
  "name": "agentmodbus",
  "email": "mindspherebasic@126.com",
  "version": "21.38.2",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=develop pm2 --no-daemon start ./bin/www --watch ",
    "stage": "cross-env NODE_ENV=stage pm2 --no-daemon start ./bin/www --watch ",
    "nodemon": "nodemon ./bin/www",
    "start": "node bin/www",
    "pm2": "pm2 start bin/www",
    "pm2-stop": "pm2 stop all",
    "build": "set NODE_ENV=production",
    "pkg": "pkg ./bin/www  -t win --out-path=dist/",
    "test": "mocha --max_old_space_size=90000 ./test/**/*.js --timeout 150000"
  },
  "bin": "bin/www",
  "main": "bin/www",
  "pkg": {
    "scripts": "routes/**/*.js",
    "assets": [
      "public/**/*",
      "views/**"
    ],
    "targets": [
      "node8-windows-x64"
    ],
    "outputPath": "pkg/"
  },
  "author": "fungi0",
  "devDependencies": {
    "chai": "^4.3.4",
    "mocha": "^7.2.0"
  },
  "dependencies": {
    "chai": "^4.3.4",
    "mocha": "^7.2.0",
    "jsmodbus": "~4.0.6",
    "net": "~1.0.2",
    "nodemailer": "~6.6.2",
    "log4js": "~6.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  }
}
